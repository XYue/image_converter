<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
            
    <PropertyGroup>
        <GDAL_CONFIG_LINK>static</GDAL_CONFIG_LINK>
        <BOOST_CONFIG_LINK>static</BOOST_CONFIG_LINK>
        <GOOGLE_GFLAGS_CONFIG_LINK>static</GOOGLE_GFLAGS_CONFIG_LINK>
    </PropertyGroup>
    
    <ImportGroup Label="PropertySheets">
        <Import
            Project="$(MSBuildThisFileDirectory)..\..\..\..\..\..\Shared\gdal\kit\gdal_latest.props"
            Condition="'$(GDAL_IMPORT)'!='true'" />
        <Import
            Project="$(MSBuildThisFileDirectory)..\..\..\..\..\..\Shared\boost\kit\boost_latest.props"
            Condition="'$(BOOST_IMPORT)'!='true'" />
        <Import
            Project="$(MSBuildThisFileDirectory)..\..\..\..\..\..\Shared\google\gflags\kit\google_gflags_latest.props"
            Condition="'$(GOOGLE_GFLAGS_IMPORT)'!='true'" />
    </ImportGroup>
    
</Project>
